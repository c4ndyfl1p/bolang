{
  "manifest_version": 3,
  "name": "Noun Translator",
  "version": "1.0",
  "description": "Replaces nouns with translations.",
  "permissions": ["scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["http://localhost:5000/"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
